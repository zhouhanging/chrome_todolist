{
  "name": "todoList",
  "description": "工作待办事项管理工具",
  "version": "1.0.1",
  "manifest_version": 2,
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "browser_action": {
    "default_popup": "popup.html" 
  },
  "background": {
    "scripts": [
      "background.js" 
    ]
  },
  "permissions": ["contextMenus", "notifications"]
}
